{
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch - Client",
			"url": "http://localhost:4200",
			"webRoot": "${workspaceFolder}",
			"preLaunchTask": "start",
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "chrome",
			"request": "attach",
			"port": 4200,
			"name": "Attach - Client",
			"url": "http://localhost:4200",
			"webRoot": "${workspaceFolder}"
		}
	]
}
